{
    "manifest_version": 1,
    
    "name": "github_memo",
    "description": "simple memo to repository of github",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}
